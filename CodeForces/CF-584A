#include <iostream>
using namespace std;
 
int main() {
	int n,t;
	cin>>n>>t;
	if(n==1 && t<10) cout<<t;
	else if(n==1 && t>=10) cout<<-1;
	else{
		if(t==2){
			string s="1";
			for(int i=0;i<n-2;i++)s=s+'0';
			s=s+'2';
			cout<<s;
		}
		else if(t==3){
			string s="3";
			for(int i=0;i<n-2;i++)s=s+'0';
			s=s+'3';
			cout<<s;
		}
		else if(t==4){
			string s="4";
			for(int i=0;i<n-2;i++)s=s+'0';
			s=s+'4';
			cout<<s;
		}
		else if(t==5){
			string s="5";
			for(int i=0;i<n-2;i++)s=s+'0';
			s=s+'5';
			cout<<s;
		}
		else if(t==6){
			string s="6";
			for(int i=0;i<n-2;i++)s=s+'0';
			s=s+'6';
			cout<<s;
		}else if(t==7){
			string s="7";
			for(int i=0;i<n-2;i++)s=s+'0';
			s=s+'7';
			cout<<s;
		}
		else if(t==8){
			string s="8";
			for(int i=0;i<n-2;i++)s=s+'0';
			s=s+'8';
			cout<<s;
		}
		else if(t==9){
			string s="9";
			for(int i=0;i<n-2;i++)s=s+'0';
			s=s+'9';
			cout<<s;
		}
		else if(t==10){
			string s="10";
			for(int i=0;i<n-2;i++)s=s+'0';
			cout<<s;
		}
		
	}
	return 0;
}
